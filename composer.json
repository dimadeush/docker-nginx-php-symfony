{
	"name": "dimadeush/docker-nginx-php-symfony",
	"type": "project",
	"description": "Docker symfony environment",
	"keywords": [
		"Docker",
		"Nginx",
		"Php",
		"Symfony",
		"Supervisord",
		"MySQL",
		"RabbitMQ"
	],
	"homepage": "https://github.com/dimadeush/docker-nginx-php-symfony",
	"version": "1.0.0",
	"license": "proprietary",
	"authors": [
		{
			"name": "Dmitriy Kravtsov",
			"email": "dimitriy_k@ukr.net",
			"homepage": "https://github.com/dimadeush",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/dimadeush/docker-nginx-php-symfony/issues"
	},
	"prefer-stable": true,
	"require": {
		"php": "^7.4.0",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-pdo": "*",
		"ext-pdo_mysql": "*",
		"doctrine/doctrine-migrations-bundle": "^2.1",
		"jmose/command-scheduler-bundle": "^2.0",
		"sensio/framework-extra-bundle": "^5.5",
		"sensiolabs/security-checker": "^6.0",
		"symfony/amqp-pack": "^1.0",
		"symfony/asset": "4.4.*",
		"symfony/config": "4.4.*",
		"symfony/console": "4.4.*",
		"symfony/dotenv": "4.4.*",
		"symfony/expression-language": "4.4.*",
		"symfony/flex": "^1.6",
		"symfony/form": "4.4.*",
		"symfony/framework-bundle": "4.4.*",
		"symfony/http-client": "4.4.*",
		"symfony/intl": "4.4.*",
		"symfony/mailer": "4.4.*",
		"symfony/messenger": "4.4.*",
		"symfony/monolog-bundle": "^3.5",
		"symfony/orm-pack": "*",
		"symfony/process": "4.4.*",
		"symfony/routing": "4.4.*",
		"symfony/security-bundle": "4.4.*",
		"symfony/serializer-pack": "*",
		"symfony/translation": "4.4.*",
		"symfony/twig-bundle": "4.4.*",
		"symfony/validator": "4.4.*",
		"symfony/web-link": "4.4.*",
		"symfony/yaml": "4.4.*"
	},
	"require-dev": {
		"bamarni/composer-bin-plugin": "^1.3",
		"doctrine/doctrine-fixtures-bundle": "^3.3",
		"roave/security-advisories": "dev-master",
		"symfony/debug-pack": "*",
		"symfony/maker-bundle": "^1.14",
		"symfony/profiler-pack": "*",
		"symfony/test-pack": "*"
	},
	"config": {
		"platform": {
			"php": "7.4.0"
		},
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},
	"replace": {
		"paragonie/random_compat": "2.*",
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php72": "*",
		"symfony/polyfill-php71": "*",
		"symfony/polyfill-php70": "*",
		"symfony/polyfill-php56": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd",
			"security-checker security:check": "script"
		},
		"post-install-cmd": [
			"if test -d vendor/bamarni/composer-bin-plugin; then composer bin all install; fi",
			"@auto-scripts"
		],
		"post-update-cmd": [
			"if test -d vendor/bamarni/composer-bin-plugin; then composer bin all update; fi",
			"@auto-scripts"
		]
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"extra": {
		"bamarni-bin": {
			"target-directory": "tools"
		},
		"symfony": {
			"allow-contrib": true,
			"require": "4.4.*"
		}
	}
}
